				Catch 
				{

					$PSCmdlet.ThrowTerminatingError($_)

				}

			}

		}

	}

	Process 
	{

		#Get Source Volume resource
		Switch ($InputObject.GetType().Name) 
		{

			"String" 
			{ 
				 
				#Parameter is correct URI
				if ($InputObject.StartsWith($StorageVolumeTemplatesUri))
				{

