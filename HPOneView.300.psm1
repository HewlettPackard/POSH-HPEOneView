function Get-HPOVEnclosureGroup 
{
	
	# .ExternalHelp HPOneView.300.psm1-help.xml

	[CmdletBinding()]    
	Param 
	(

		[Parameter (Mandatory = $false)]
		[ValidateNotNullorEmpty()]
		[string]$Name,

		[Parameter (Mandatory = $false)]
