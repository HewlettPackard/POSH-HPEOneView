					$_keys = Send-HPOVRequest $RabbitMQKeyPairUri -Hostname $_appliance.Name

				}

				Catch
				{
			
					$PSCmdlet.ThrowTerminatingError($_)
			
				}

			}

			Catch
			{

				$PSCmdlet.ThrowTerminatingError($_)

			}
		
			try 
			{

				New-Item $PrivateKeyFile -type file -force -value $_keys.base64SSLKeyData | write-verbose

