		}

		Catch
		{

			$PSCmdlet.ThrowTerminatingError($_)

		}

		$r.members | % {

			[void]$_IndexSearchResults.Add($_)

		}

	}
	
	end
	{

		Return $_IndexSearchResults

	}

}

