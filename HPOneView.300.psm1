			}
			
			Catch
			{

				$PSCmdlet.ThrowTerminatingError($_)

			}

			$_resp.PSObject.TypeNames.Insert(0,'HPOneView.Appliance.AuthDirectoryGroupRoleMapping')

			[void]$_DirectroyGroupStatus.Add($_resp)
		   
		}

	}

	End
	{
		
		Return $_DirectroyGroupStatus

	}

