function New-HPOVCustomBaseline
{

	# .ExternalHelp HPOneView.300.psm1-help.xml

	[CmdletBinding()]
	Param 
	(

		[Parameter (Mandatory, ValueFromPipeline)]
		[ValidateNotNullorEmpty()]
		[Object]$SourceBaseline,

		[Parameter (Mandatory)]
