			{

				$_resp = Send-HPOVRequest $usersUri POST $_user -Hostname $_appliance.Name

			}
			
			Catch
			{

				$PSCmdlet.ThrowTerminatingError($_)

			}

			$_resp.PSObject.TypeNames.Insert(0,'HPOneView.Appliance.User')

			[void]$_UserStatus.Add($_resp)

		}

	}

	End
	{
		
		Return $_UserStatus

	}

