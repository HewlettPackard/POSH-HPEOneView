{

	# .ExternalHelp HPOneView.300.psm1-help.xml

	[CmdLetBinding(DefaultParameterSetName = 'Default')]
	Param 
	(

		[Parameter (Mandatory = $false, ParameterSetName = 'Default')]
		[ValidateNotNullorEmpty()]
		[String]$Name,

