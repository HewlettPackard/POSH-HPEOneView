				{

					$_resp = Send-HPOVRequest ($powerDevicePotentialConnections + $PowerDevice.uri)

				}

				Catch
				{

					$PSCmdlet.ThrowTerminatingError($_)

				}				

				if ($_resp)
				{

					$_resp | % { 
						
						$_.PSObject.TypeNames.Insert(0,'HPOneView.PowerDevice.PotentialPowerConnection')
					
