		}

		elseif ($Password -is [SecureString])
		{

			$_decryptPassword = [Runtime.InteropServices.Marshal]::PtrToStringAuto([Runtime.InteropServices.Marshal]::SecureStringToBSTR($Password))

		}

		else 
		{

			$_decryptPassword = $Password

		}

	}

