                <ScriptBlock>
                  switch ($_.type)
                  {
                  
                    'sas-logical-interconnect-group'
                    {
                    
                      'SAS'
                      
                    }
                    
                    default
                    {
                    
                      'VC'
                    
                    }
                  
                  }
                </ScriptBlock>
              </TableColumnItem>
